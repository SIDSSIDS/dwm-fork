#!/bin/bash

source $(dirname $0)/colors

muted=$(pamixer --get-mute)

if [[ "$1" == "sym" ]]
then
    if [[ "$muted" == "true" ]]
    then
        echo -n ""
    else
        echo -n ""
    fi
else
    if [[ "$muted" == "true" ]]
    then
        echo -n "$colRed500"
    else
        echo -n "$colYellow500"
    fi
fi

